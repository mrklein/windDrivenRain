FoamFile
{
   version     2.0;
   format      ascii;
   class       volScalarField;
   object      alpha;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //


dimensions    [0 0 0 0 0 0 0];


internalField uniform 0;


boundaryField
{
   inlet
   {
       type    fixedValue;
       value   uniform 4.43371674519705e-09;
   }
   top
   {
       type    fixedValue;
       value   uniform 4.43371674519705e-09;
   }

   outlet
   {
       type    inletOutlet;
       inletValue    uniform 0;
       value    uniform 0;
   }
   ground
   {
       type    inletOutlet;
       inletValue    uniform 0;
       value    uniform 0;
   }
   cube
   {
       type    inletOutlet;
       inletValue    uniform 0;
       value    uniform 0;
   }
   sides
   {
       type    slip;
   }
}

